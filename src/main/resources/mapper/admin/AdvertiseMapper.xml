<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.goodcub.vci.mapper.AdvertiseMapper">

    <select id="queryAdvertiseList" resultType="com.goodcub.vci.entity.Advertise" parameterType="java.util.Map">
        select * from advertise
        <where>
            <if test="adtitle!=null">
                adtitle like concat('%',#{adtitle},'%')
            </if>
        </where>
    </select>

</mapper>
